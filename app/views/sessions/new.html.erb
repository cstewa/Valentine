<p id="notice"><%= notice %></p>


<h1>Login</h1>

<%= form_tag({controller: "sessions", action: "create"},method: "post") do %>
  <%= label_tag "Email:" %>
  <%= text_field_tag :email %><br>

  <%= label_tag :password %>
  <%= password_field_tag :password %><br>

  <%= submit_tag("Log In") %>
<% end %>

